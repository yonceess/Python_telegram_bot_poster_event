import telebot
from telebot import types

answer = ''
month = ''
year = ''
date = ''
wait = ''
ans = ''

bot = telebot.TeleBot("1418591012:AAG04nXes6sUSsKYou0FhfoprDm6dHyh4u0")


@bot.message_handler(commands=['start'])
def start_message(message):
    bot.send_message(message.chat.id, """
    –î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å!üòä 
–ï—Å–ª–∏ –≤—ã —Ö–æ—Ç–∏—Ç–µ –æ–∑–Ω–∞–∫–æ–º–∏—Ç—å—Å—è —Å –±–ª–∏–∂–∞–π—à–∏–º —Ä–∞—Å–ø–∏—Å–∞–Ω–∏–µ–º –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏–π,–Ω–∞–ø–∏—à–∏—Ç–µ '–ü—Ä–∏–≤–µ—Ç'.
–ü–∞–º—è—Ç–∫–∞:
1‚É£ –ü–∏—à–∏ –Ω–∞–∑–≤–∞–Ω–∏—è –≥–æ—Ä–æ–¥–æ–≤ –∏ –º–µ—Å—è—Ü–µ–≤ —Å –∑–∞–≥–ª–∞–≤–Ω–æ–π –±—É–∫–≤—ã.
2‚É£ –ù–∞ –¥–∞–Ω–Ω—ã–π –º–æ–º–µ–Ω—Ç —è –ø—Ä–µ–¥—Å—Ç–∞–≤–ª—è—é –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏—è —Å –ù–æ—è–±—Ä—è 2020 –≥–æ–¥–∞ –¥–æ –ò—é–Ω—è 2021 –≥–æ–¥–∞.
3‚É£ –° –í–∞–º–∏ —Å–≤—è–∂–µ—Ç—Å—è –Ω–∞—à –º–µ–Ω–µ–¥–∂–µ—Ä –ø–æ—Å–ª–µ —Å–æ–≤–µ—Ä—à–µ–Ω–∏—è –ø–æ–∫—É–ø–∫–∏ –≤ —Ç–µ—á–µ–Ω–∏–µ –¥–Ω—è. """, parse_mode="Markdown")
    bot.register_next_step_handler(message, get_text_messages)


@bot.message_handler(commands=['help'])
def send_welcome(message):
    if message.text == "/help":
        bot.send_message(message.from_user.id, "–ù–∞–ø–∏—à–∏ –ø—Ä–∏–≤–µ—Ç")
    else:
        bot.send_message(message.from_user.id, "–Ø —Ç–µ–±—è –Ω–µ –ø–æ–Ω–∏–º–∞—é. –ù–∞–ø–∏—à–∏ /help.")


@bot.message_handler(content_types=['text', 'document', 'audio'])
def get_text_messages(message):
    if message.text == '–ü—Ä–∏–≤–µ—Ç' or message.text == '–ø—Ä–∏–≤–µ—Ç':
        bot.send_message(message.from_user.id, "–ü—Ä–∏–≤–µ—Ç! –í—ã —Ö–æ—Ç–∏—Ç–µ –æ–∑–Ω–∞–∫–æ–º–∏—Ç—å—Å—è —Å –∞—Ñ–∏—à–µ–π –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏–π?")
        keyboard = telebot.types.ReplyKeyboardMarkup(True)
        keyboard.row('–î–∞', '–ù–µ—Ç')
        bot.send_message(message.chat.id, '–í—ã–±–µ—Ä–∏—Ç–µ –æ–ø—Ü–∏—é –Ω–∞ –∫–ª–∞–≤–∏–∞—Ç—É—Ä–µ:',
                         reply_markup=keyboard)
        bot.register_next_step_handler(message, reg_answer)
    else:
        bot.send_message(message.from_user.id, "–Ø —Ç–µ–±—è –Ω–µ –ø–æ–Ω–∏–º–∞—é. –ù–∞–ø–∏—à–∏ /help.")


def reg_answer(message):
    global answer
    answer = message.text
    if message.text == '–ù–µ—Ç' or message.text == '–Ω–µ—Ç':
        bot.reply_to(message, '–ò–∑–≤–∏–Ω–∏—Ç–µ, –µ—Å–ª–∏ —É –≤–∞—Å –±—É–¥–µ—Ç –∂–µ–ª–∞–Ω–∏–µ –æ–∑–Ω–∞–∫–æ–º–∏—Ç—å—Å—è —Å –∞—Ñ–∏—à–µ–π - –ø–∏—à–∏—Ç–µ /start')
    elif message.text == "/help":
        bot.send_message(message.from_user.id, "–ù–∞–ø–∏—à–∏ –ø—Ä–∏–≤–µ—Ç")
    elif message.text == "/start":
        bot.send_message(message.chat.id, """
            –î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å!üòä 
        –ï—Å–ª–∏ –≤—ã —Ö–æ—Ç–∏—Ç–µ –æ–∑–Ω–∞–∫–æ–º–∏—Ç—å—Å—è —Å –±–ª–∏–∂–∞–π—à–∏–º —Ä–∞—Å–ø–∏—Å–∞–Ω–∏–µ–º –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏–π,–Ω–∞–ø–∏—à–∏—Ç–µ '–ü—Ä–∏–≤–µ—Ç'.
        –ü–∞–º—è—Ç–∫–∞:
        1‚É£ –ü–∏—à–∏ –Ω–∞–∑–≤–∞–Ω–∏—è –≥–æ—Ä–æ–¥–æ–≤ –∏ –º–µ—Å—è—Ü–µ–≤ —Å –∑–∞–≥–ª–∞–≤–Ω–æ–π –±—É–∫–≤—ã.
        2‚É£ –ù–∞ –¥–∞–Ω–Ω—ã–π –º–æ–º–µ–Ω—Ç —è –ø—Ä–µ–¥—Å—Ç–∞–≤–ª—è—é –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏—è —Å –ù–æ—è–±—Ä—è 2020 –≥–æ–¥–∞ –¥–æ –ò—é–Ω—è 2021 –≥–æ–¥–∞.
        3‚É£ –° –í–∞–º–∏ —Å–≤—è–∂–µ—Ç—Å—è –Ω–∞—à –º–µ–Ω–µ–¥–∂–µ—Ä –ø–æ—Å–ª–µ —Å–æ–≤–µ—Ä—à–µ–Ω–∏—è –ø–æ–∫—É–ø–∫–∏ –≤ —Ç–µ—á–µ–Ω–∏–µ –¥–Ω—è. """, parse_mode="Markdown")
        bot.register_next_step_handler(message.from_user.id, get_text_messages)
    else:
        bot.send_message(message.from_user.id, "–ê—Ñ–∏—à–∞ –∫–∞–∫–æ–≥–æ –≥–æ—Ä–æ–¥–∞ –≤–∞—Å –∏–Ω—Ç–µ—Ä–µ—Å—É–µ—Ç?")
    bot.register_next_step_handler(message, reg_month)


def reg_month(message):
    global month
    month = message.text
    if message.text == "/help":
        bot.send_message(message.from_user.id, "–ù–∞–ø–∏—à–∏ –ø—Ä–∏–≤–µ—Ç")
    elif message.text == "/start":
        bot.send_message(message.chat.id, """
            –î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å!üòä 
        –ï—Å–ª–∏ –≤—ã —Ö–æ—Ç–∏—Ç–µ –æ–∑–Ω–∞–∫–æ–º–∏—Ç—å—Å—è —Å –±–ª–∏–∂–∞–π—à–∏–º —Ä–∞—Å–ø–∏—Å–∞–Ω–∏–µ–º –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏–π,–Ω–∞–ø–∏—à–∏—Ç–µ '–ü—Ä–∏–≤–µ—Ç'.
        –ü–∞–º—è—Ç–∫–∞:
        1‚É£ –ü–∏—à–∏ –Ω–∞–∑–≤–∞–Ω–∏—è –≥–æ—Ä–æ–¥–æ–≤ –∏ –º–µ—Å—è—Ü–µ–≤ —Å –∑–∞–≥–ª–∞–≤–Ω–æ–π –±—É–∫–≤—ã.
        2‚É£ –ù–∞ –¥–∞–Ω–Ω—ã–π –º–æ–º–µ–Ω—Ç —è –ø—Ä–µ–¥—Å—Ç–∞–≤–ª—è—é –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏—è —Å –ù–æ—è–±—Ä—è 2020 –≥–æ–¥–∞ –¥–æ –ò—é–Ω—è 2021 –≥–æ–¥–∞.
        3‚É£ –° –í–∞–º–∏ —Å–≤—è–∂–µ—Ç—Å—è –Ω–∞—à –º–µ–Ω–µ–¥–∂–µ—Ä –ø–æ—Å–ª–µ —Å–æ–≤–µ—Ä—à–µ–Ω–∏—è –ø–æ–∫—É–ø–∫–∏ –≤ —Ç–µ—á–µ–Ω–∏–µ –¥–Ω—è. """, parse_mode="Markdown")
        bot.register_next_step_handler(message.from_user.id, get_text_messages)
    else:
        bot.send_message(message.from_user.id, "–í—ã–±–µ—Ä–∏—Ç–µ –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã–π –º–µ—Å—è—Ü")
    bot.register_next_step_handler(message, reg_year)


def reg_year(message):
    global year
    year = message.text
    if message.text == "/help":
        bot.send_message(message.from_user.id, "–ù–∞–ø–∏—à–∏ –ø—Ä–∏–≤–µ—Ç")
    elif message.text == "/start":
        bot.send_message(message.chat.id, """
                    –î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å!üòä 
        –ï—Å–ª–∏ –≤—ã —Ö–æ—Ç–∏—Ç–µ –æ–∑–Ω–∞–∫–æ–º–∏—Ç—å—Å—è —Å –±–ª–∏–∂–∞–π—à–∏–º —Ä–∞—Å–ø–∏—Å–∞–Ω–∏–µ–º –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏–π,–Ω–∞–ø–∏—à–∏—Ç–µ '–ü—Ä–∏–≤–µ—Ç'.
        –ü–∞–º—è—Ç–∫–∞:
        1‚É£ –ü–∏—à–∏ –Ω–∞–∑–≤–∞–Ω–∏—è –≥–æ—Ä–æ–¥–æ–≤ –∏ –º–µ—Å—è—Ü–µ–≤ —Å –∑–∞–≥–ª–∞–≤–Ω–æ–π –±—É–∫–≤—ã.
        2‚É£ –ù–∞ –¥–∞–Ω–Ω—ã–π –º–æ–º–µ–Ω—Ç —è –ø—Ä–µ–¥—Å—Ç–∞–≤–ª—è—é –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏—è —Å –ù–æ—è–±—Ä—è 2020 –≥–æ–¥–∞ –¥–æ –ò—é–Ω—è 2021 –≥–æ–¥–∞.
        3‚É£ –° –í–∞–º–∏ —Å–≤—è–∂–µ—Ç—Å—è –Ω–∞—à –º–µ–Ω–µ–¥–∂–µ—Ä –ø–æ—Å–ª–µ —Å–æ–≤–µ—Ä—à–µ–Ω–∏—è –ø–æ–∫—É–ø–∫–∏ –≤ —Ç–µ—á–µ–Ω–∏–µ –¥–Ω—è. """, parse_mode="Markdown")
        bot.register_next_step_handler(message.from_user.id, get_text_messages)
    else:
        bot.send_message(message.from_user.id, "–ú–µ—Ä–æ–ø—Ä–∏—è—Ç–∏—è –∫–∞–∫–æ–≥–æ –≥–æ–¥–∞ –≤—ã —Ö–æ—Ç–∏—Ç–µ –ø—Ä–æ—Å–º–æ—Ç—Ä–µ—Ç—å?")
    bot.register_next_step_handler(message, reg_date)


def reg_date(message):
    global date
    date = message.text
    if message.text == "/help":
        bot.send_message(message.from_user.id, "–ù–∞–ø–∏—à–∏ –ø—Ä–∏–≤–µ—Ç")
    elif message.text == "/start":
        bot.send_message(message.chat.id, """
                            –î–æ–±—Ä–æ –ø–æ–∂–∞–ª–æ–≤–∞—Ç—å!üòä 
                –ï—Å–ª–∏ –≤—ã —Ö–æ—Ç–∏—Ç–µ –æ–∑–Ω–∞–∫–æ–º–∏—Ç—å—Å—è —Å –±–ª–∏–∂–∞–π—à–∏–º —Ä–∞—Å–ø–∏—Å–∞–Ω–∏–µ–º –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏–π,–Ω–∞–ø–∏—à–∏—Ç–µ '–ü—Ä–∏–≤–µ—Ç'.
                –ü–∞–º—è—Ç–∫–∞:
                1‚É£ –ü–∏—à–∏ –Ω–∞–∑–≤–∞–Ω–∏—è –≥–æ—Ä–æ–¥–æ–≤ –∏ –º–µ—Å—è—Ü–µ–≤ —Å –∑–∞–≥–ª–∞–≤–Ω–æ–π –±—É–∫–≤—ã.
                2‚É£ –ù–∞ –¥–∞–Ω–Ω—ã–π –º–æ–º–µ–Ω—Ç —è –ø—Ä–µ–¥—Å—Ç–∞–≤–ª—è—é –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏—è —Å –ù–æ—è–±—Ä—è 2020 –≥–æ–¥–∞ –¥–æ –ò—é–Ω—è 2021 –≥–æ–¥–∞.
                3‚É£ –° –í–∞–º–∏ —Å–≤—è–∂–µ—Ç—Å—è –Ω–∞—à –º–µ–Ω–µ–¥–∂–µ—Ä –ø–æ—Å–ª–µ —Å–æ–≤–µ—Ä—à–µ–Ω–∏—è –ø–æ–∫—É–ø–∫–∏ –≤ —Ç–µ—á–µ–Ω–∏–µ –¥–Ω—è. """, parse_mode="Markdown")
        bot.register_next_step_handler(message.from_user.id, get_text_messages)
    else:
        global wait
        while wait == 0:
            # noinspection PyBroadException
            try:
                wait = int(message.text)
            except Exception:
                bot.send_message(message.from_user.id, "–ò–¥–µ—Ç –ø—Ä–æ—Ü–µ—Å—Å –æ–±—Ä–∞–±–∞—Ç—ã–≤–∞–Ω–∏—è –∏–Ω—Ñ–æ—Ä–º–∞—Ü–∏–∏.")
        keyboard = types.InlineKeyboardMarkup()
        key_yes = types.InlineKeyboardButton(text='–î–∞', callback_data='yes')
        keyboard.add(key_yes)
        key_no = types.InlineKeyboardButton(text='–ù–µ—Ç', callback_data='no')
        keyboard.add(key_no)
        question = '–í–∞–º –Ω–µ–æ–±—Ö–æ–¥–∏–º—ã –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏—è ' + date + ' –≥–æ–¥–∞. –í –≥–æ—Ä–æ–¥–µ ' + month + '' + '?'
        bot.send_message(message.from_user.id, text=question, reply_markup=keyboard)


@bot.callback_query_handler(func=lambda call: True)
def callback_worker(call):
    if call.data == "yes":
        bot.send_message(call.message.chat.id, "–ü–æ–¥—Ç–≤–µ—Ä–¥–∏—Ç–µ —Å–≤–æ–π –∑–∞–ø—Ä–æ—Å, –Ω–∞–ø–∏—Å–∞–≤ '–î–∞'.‚¨á")
        bot.register_next_step_handler(call.message, send_welcome)
    elif call.data == "no":
        bot.send_message(call.message.chat.id, "–ü–æ–ø—Ä–æ–±—É–µ–º –µ—â–µ —Ä–∞–∑!")
        bot.send_message(call.message.chat.id, "–ü—Ä–∏–≤–µ—Ç! –í—ã —Ö–æ—Ç–∏—Ç–µ –æ–∑–Ω–∞–∫–æ–º–∏—Ç—å—Å—è —Å –∞—Ñ–∏—à–µ–π –º–µ—Ä–æ–ø—Ä–∏—è—Ç–∏–π?")
        bot.register_next_step_handler(call.message, reg_answer)
    elif call.data == 'first':
        key3 = types.InlineKeyboardMarkup()
        key_first = types.InlineKeyboardButton(text='–í–ò–ü', callback_data='VIP')
        key_second = types.InlineKeyboardButton(text='–°—Ç–∞–Ω–¥–∞—Ä—Ç', callback_data='stan')
        key3.add(key_first)
        key3.add(key_second)
        bot.send_message(call.message.chat.id, "–û—Ç–ª–∏—á–Ω—ã–π –≤—ã–±–æ—Ä!")
        bot.send_message(call.from_user.id, '–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø –±–∏–ª–µ—Ç–∞:', reply_markup=key3)
    elif call.data == 'second':
        bot.send_message(call.message.chat.id, "–û—Ç–ª–∏—á–Ω—ã–π –≤—ã–±–æ—Ä!")
        key3 = types.InlineKeyboardMarkup()
        key_first = types.InlineKeyboardButton(text='–í–ò–ü', callback_data='VIP')
        key_second = types.InlineKeyboardButton(text='–°—Ç–∞–Ω–¥–∞—Ä—Ç', callback_data='stan')
        key3.row(key_first)
        key3.row(key_second)
        bot.send_message(call.from_user.id, '–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø –±–∏–ª–µ—Ç–∞:', reply_markup=key3)
    elif call.data == 'third':
        bot.send_message(call.message.chat.id, "–û—Ç–ª–∏—á–Ω—ã–π –≤—ã–±–æ—Ä!")
        key3 = types.InlineKeyboardMarkup()
        key_first = types.InlineKeyboardButton(text='–í–ò–ü', callback_data='VIP')
        key_second = types.InlineKeyboardButton(text='–°—Ç–∞–Ω–¥–∞—Ä—Ç', callback_data='stan')
        key3.row(key_first)
        key3.row(key_second)
        bot.send_message(call.from_user.id, '–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø –±–∏–ª–µ—Ç–∞:', reply_markup=key3)
    elif call.data == 'forth':
        bot.send_message(call.message.chat.id, "–û—Ç–ª–∏—á–Ω—ã–π –≤—ã–±–æ—Ä!")
        key3 = types.InlineKeyboardMarkup()
        key_first = types.InlineKeyboardButton(text='–í–ò–ü', callback_data='VIP')
        key_second = types.InlineKeyboardButton(text='–°—Ç–∞–Ω–¥–∞—Ä—Ç', callback_data='stan')
        key3.row(key_first)
        key3.row(key_second)
        bot.send_message(call.from_user.id, '–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø –±–∏–ª–µ—Ç–∞:', reply_markup=key3)
    elif call.data == 'fifth':
        bot.send_message(call.message.chat.id, "–û—Ç–ª–∏—á–Ω—ã–π –≤—ã–±–æ—Ä!")
        key3 = types.InlineKeyboardMarkup()
        key_first = types.InlineKeyboardButton(text='–í–ò–ü', callback_data='VIP')
        key_second = types.InlineKeyboardButton(text='–°—Ç–∞–Ω–¥–∞—Ä—Ç', callback_data='stan')
        key3.row(key_first)
        key3.row(key_second)
        bot.send_message(call.from_user.id, '–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø –±–∏–ª–µ—Ç–∞:', reply_markup=key3)
    elif call.data == 'sixth':
        bot.send_message(call.message.chat.id, "–û—Ç–ª–∏—á–Ω—ã–π –≤—ã–±–æ—Ä!")
        key3 = types.InlineKeyboardMarkup()
        key_first = types.InlineKeyboardButton(text='–í–ò–ü', callback_data='VIP')
        key_second = types.InlineKeyboardButton(text='–°—Ç–∞–Ω–¥–∞—Ä—Ç', callback_data='stan')
        key3.row(key_first)
        key3.row(key_second)
        bot.send_message(call.from_user.id, '–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø –±–∏–ª–µ—Ç–∞:', reply_markup=key3)
    elif call.data == 'seventh':
        bot.send_message(call.message.chat.id, "–û—Ç–ª–∏—á–Ω—ã–π –≤—ã–±–æ—Ä!")
        key3 = types.InlineKeyboardMarkup()
        key_first = types.InlineKeyboardButton(text='–í–ò–ü', callback_data='VIP')
        key_second = types.InlineKeyboardButton(text='–°—Ç–∞–Ω–¥–∞—Ä—Ç', callback_data='stan')
        key3.row(key_first)
        key3.row(key_second)
        bot.send_message(call.from_user.id, '–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø –±–∏–ª–µ—Ç–∞:', reply_markup=key3)
    elif call.data == 'eight':
        bot.send_message(call.message.chat.id, "–û—Ç–ª–∏—á–Ω—ã–π –≤—ã–±–æ—Ä!")
        key3 = types.InlineKeyboardMarkup()
        key_first = types.InlineKeyboardButton(text='–í–ò–ü', callback_data='VIP')
        key_second = types.InlineKeyboardButton(text='–°—Ç–∞–Ω–¥–∞—Ä—Ç', callback_data='stan')
        key3.row(key_first)
        key3.row(key_second)
        bot.send_message(call.from_user.id, '–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø –±–∏–ª–µ—Ç–∞:', reply_markup=key3)
    elif call.data == 'nine':
        bot.send_message(call.message.chat.id, "–û—Ç–ª–∏—á–Ω—ã–π –≤—ã–±–æ—Ä!")
        key3 = types.InlineKeyboardMarkup()
        key_first = types.InlineKeyboardButton(text='–í–ò–ü', callback_data='VIP')
        key_second = types.InlineKeyboardButton(text='–°—Ç–∞–Ω–¥–∞—Ä—Ç', callback_data='stan')
        key3.row(key_first)
        key3.row(key_second)
        bot.send_message(call.from_user.id, '–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø –±–∏–ª–µ—Ç–∞:', reply_markup=key3)
    elif call.data == 'ten':
        bot.send_message(call.message.chat.id, "–û—Ç–ª–∏—á–Ω—ã–π –≤—ã–±–æ—Ä!")
        key3 = types.InlineKeyboardMarkup()
        key_first = types.InlineKeyboardButton(text='–í–ò–ü', callback_data='VIP')
        key_second = types.InlineKeyboardButton(text='–°—Ç–∞–Ω–¥–∞—Ä—Ç', callback_data='stan')
        key3.row(key_first)
        key3.row(key_second)
        bot.send_message(call.from_user.id, '–í—ã–±–µ—Ä–∏—Ç–µ —Ç–∏–ø –±–∏–ª–µ—Ç–∞:', reply_markup=key3)
    elif call.data == 'VIP':
        key68 = types.InlineKeyboardMarkup()
        key_first = types.InlineKeyboardButton(text='–°–≤—è–∑–∞—Ç—å—Å—è —Å –º–µ–Ω–µ–¥–∂–µ—Ä–æ–º ‚úÖ', callback_data='yees')
        key_second = types.InlineKeyboardButton(text='–û—Ç–º–µ–Ω–∞ ‚ùå', callback_data='no')
        key68.row(key_first)
        key68.row(key_second)
        bot.edit_message_text(
            "–î–∞–Ω–Ω—ã–µ –º–µ–Ω–µ–¥–∂–µ—Ä–∞: –ö–∞–π—ã—Ä–æ–≤ –î–∞—Å—Ç–∞–Ω, +77777777777. "
            "C–ø–∞—Å–∏–±–æ, —á—Ç–æ –ø–æ–ª—å–∑—É–µ—Ç–µ—Å—å –Ω–∞—à–∏–º —Å–µ—Ä–≤–∏—Å–æ–º!üòä",
            call.message.chat.id, call.message.message_id, reply_markup=key68)
    elif call.data == 'stan':
        key68 = types.InlineKeyboardMarkup()
        key_first = types.InlineKeyboardButton(text='–°–≤—è–∑–∞—Ç—å—Å—è —Å –º–µ–Ω–µ–¥–∂–µ—Ä–æ–º ‚úÖ', callback_data='yees')
        key_second = types.InlineKeyboardButton(text='–û—Ç–º–µ–Ω–∞ ‚ùå', callback_data='nope')
        key68.row(key_first)
        key68.row(key_second)
        bot.edit_message_text(
            "–î–∞–Ω–Ω—ã–µ –º–µ–Ω–µ–¥–∂–µ—Ä–∞: –ö–∞–π—ã—Ä–æ–≤ –î–∞—Å—Ç–∞–Ω, +77777777777. "
            "C–ø–∞—Å–∏–±–æ, —á—Ç–æ –ø–æ–ª—å–∑—É–µ—Ç–µ—Å—å –Ω–∞—à–∏–º —Å–µ—Ä–≤–∏—Å–æ–º!üòä",
            call.message.chat.id, call.message.message_id, reply_markup=key68)
    elif call.data == 'yees':
        bot.send_message(call.message.chat.id, "–°–ø–∞—Å–∏–±–æ –∑–∞ –≤—ã–±–æ—Ä!‚ò∫")
        bot.send_message(call.message.chat.id, "–ï—Å–ª–∏ —Ö–æ—Ç–∏—Ç–µ –µ—â–µ —Ä–∞–∑ –ø–æ—Å–º–æ—Ç—Ä–µ—Ç—å —Å–ø–∏—Å–æ–∫ –∫–æ–Ω—Ü–µ—Ä—Ç–æ–≤, –Ω–∞–ø–∏—à–∏—Ç–µ /start")
    elif call.data == 'nope':
        bot.send_message(call.message.chat.id, "–°–ø–∞—Å–∏–±–æ! –•–æ—Ä–æ—à–µ–≥–æ –¥–Ω—è!üåü")


@bot.message_handler(content_types=['text', 'document', 'audio'])
def send_welcome(message):
    if month == "–ê–ª–º–∞—Ç—ã" and date == "2020" and year == "–ù–æ—è–±—Ä—å":
        bot.send_message(message.from_user.id, """\r
       –¢–≤–æ—Ä—á–µ—Å–∫–∏–π –≤–µ—á–µ—Ä —Å –ú–∞—Ä—Å–µ–ª–µ–º
       üïó–í—Ä–µ–º—è –ø—Ä–æ–≤–µ–¥–µ–Ω–∏—è: 21 –Ω–æ—è–±—Ä—è - —Å–± 19:00
       üè¢–ú–µ—Å—Ç–æ –ø—Ä–æ–≤–µ–¥–µ–Ω–∏—è: –†–µ—Å—Ç–æ—Ä–∞–Ω "–ê—Ç–º–æ—Å—Ñ–µ—Ä–∞", –ê–ª—å-–§–∞—Ä–∞–±–∏ 36
       üíµ–¶–µ–Ω—ã –Ω–∞ –±–∏–ª–µ—Ç—ã: 5 000
      [https://zakazbiletov.kz/media/events/rd/1605255605gwlkm.jpg](URL)""", parse_mode="Markdown")
